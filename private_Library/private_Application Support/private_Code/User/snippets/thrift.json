{
	// Place your snippets for thrift here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"New Service": {
		"prefix": "aurora",
		"body": [
			"aurora_lotus_internal_backstage.$1Response $1(1: aurora_lotus_internal_backstage.$1Request req) (api.get=\"/backstage/openservice/$2\")"
		],
		"description": "Log output to console"
	},
	"New platform Service": {
		"prefix": "platform",
		"body": [
			"aurora_lotus_openservice_platform.$1Response $1 (1: aurora_lotus_openservice_platform.$1Request req),"
		],
		"description": "Log output to console"
	},
	"New API": {
		"prefix": "api",
		"body": "$1Response $1 (1: $1Request req),"
	},
	"New Request Struct": {
		"prefix": "struct",
		"body": [
			"struct $1Request {",
			"    1:",
			"",
			"    255: base.Base Base,",
			"}",
			"",
			"struct $1Response {",
			"    1: ",
			"",
			"    255: base.BaseResp BaseResp,",
			"}",
		]
	}
}